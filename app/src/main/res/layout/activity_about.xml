<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".AboutActivity" >

    <RelativeLayout
        android:id="@+id/empty_view2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerInParent="true">


        <TextView
            android:id="@+id/empty_title_text2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:fontFamily="sans-serif-medium"
            android:paddingTop="36dp"
            android:textSize="30dp"
            android:text="POV BIKE DISPLAY"
            android:textAppearance="?android:textAppearanceMedium"
            />

        <TextView
            android:id="@+id/empty_subtitle_text22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/empty_title_text2"
            android:layout_centerHorizontal="true"
            android:fontFamily="sans-serif"
            android:paddingTop="8dp"
            android:text="APP MADE BY:"
            android:textAppearance="?android:textAppearanceSmall"
            android:textColor="#A2AAB0"
            />
        <TextView
            android:id="@+id/empty_subtitle_text62"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/empty_subtitle_text22"
            android:layout_centerHorizontal="true"
            android:fontFamily="sans-serif"
            android:paddingTop="8dp"
            android:text="ROHIT and RUPESH KASHYAP"
            android:textAppearance="?android:textAppearanceSmall"

            />
        <TextView
            android:id="@+id/empty_subtitle_text72"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/empty_subtitle_text62"
            android:layout_centerHorizontal="true"
            android:fontFamily="sans-serif"
            android:paddingTop="8dp"
            android:text=" for any query email us at \n rupesh21kash@gmail.com"
            android:textAppearance="?android:textAppearanceSmall"
            android:textColor="#A2AAB0"
            />
        <TextView
            android:id="@+id/empty_title_text33"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:fontFamily="sans-serif-medium"
            android:paddingTop="16dp"
            android:layout_below="@+id/empty_subtitle_text72"
            android:textSize="20dp"
            android:text="HAPPY_GLOWING"
            android:textAppearance="?android:textAppearanceMedium"
            />
    </RelativeLayout>
</RelativeLayout>